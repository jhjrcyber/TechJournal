high-availability {
    vrrp {
        group langroup9 {
            interface eth1
            virtual-address 10.0.5.1/24
            vrid 159
        }
        group wangroup9 {
            interface eth0
            virtual-address 10.0.17.79/24
            vrid 159
        }
    }
}
interfaces {
    ethernet eth0 {
        address 10.0.17.59/24
        description SEC440-WAN
        hw-id 00:50:56:b3:90:0e
    }
    ethernet eth1 {
        address 10.0.5.3/24
        description SEC440-LAN
        hw-id 00:50:56:b3:80:67
    }
    loopback lo {
    }
}
nat {
    destination {
        rule 10 {
            description "Port Forward: HTTP to Web01"
            destination {
                port 80
            }
            inbound-interface eth0
            protocol tcp
            translation {
                address 10.0.5.100
            }
        }
        rule 15 {
            description "Port Forward: SSH to Web01"
            destination {
                port 22
            }
            inbound-interface eth0
            protocol tcp
            translation {
                address 10.0.5.100
            }
        }
    }
}
protocols {
    static {
        route 0.0.0.0/0 {
            next-hop 10.0.5.1 {
            }
            next-hop 10.0.17.2 {
            }
        }
    }
}
service {
    dns {
        forwarding {
            allow-from 10.0.5.0/24
            allow-from 10.0.17.0/24
            listen-address 10.0.5.1
            listen-address 10.0.17.79
        }
    }
    ssh {
        port 22
    }
}
system {
    config-management {
        commit-revisions 100
    }
    console {
        device ttyS0 {
            speed 115200
        }
    }
    host-name fw2-joe
    login {
        user vyos {
            authentication {
                encrypted-password $6$Tf8fh/GcIL1$SZCjd50gLgpIHm3x.60qVFDKe9hhxXOtJxRVNOKmb7LFBbsvVqwTvF4nyl8HRPXfzsK6cng7xsNR4LlvL.NcY0
                plaintext-password ""
            }
        }
    }
    ntp {
        server 0.pool.ntp.org {
        }
        server 1.pool.ntp.org {
        }
        server 2.pool.ntp.org {
        }
    }
    syslog {
        global {
            facility all {
                level info
            }
            facility protocols {
                level debug
            }
        }
    }
}


